{
  "public": "frondEnd",
  "rewrites": [
    { "source": "/ads.txt", "destination": "/ads.txt" },
    { "source": "/", "destination": "/frondEnd/index.html" },
    { "source": "/(.*)", "destination": "/frondEnd/$1" }
  ],
  "headers": [
    {
      "source": "/ads.txt",
      "headers": [
        { "key": "Content-Type", "value": "text/plain" }
      ]
   }
  ]
}
